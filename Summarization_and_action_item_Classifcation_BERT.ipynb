      },
      "source": [
        "def freq(words):\n",
        "    words = [word.lower() for word in words]\n",
        "    dict_freq = {}\n",
        "    words_unique = []\n",
        "    for word in words:\n",
        "       if word not in words_unique:\n",
        "          words_unique.append(word)\n",
        "    for word in words_unique:\n",
        "          dict_freq[word] = words.count(word)\n",
        "    return dict_freq"
      ],
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "K7GJgZ-qvEI5",
