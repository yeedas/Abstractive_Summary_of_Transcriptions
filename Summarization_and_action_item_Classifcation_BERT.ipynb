    {
      "cell_type": "code",
      "metadata": {
        "id": "zbShxTJ0viSl",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "def word_tfidf(word,sentences,sentence):\n",
        "    word_tfidf = [] \n",
        "    tf = tf_score(word,sentence)\n",
        "    idf = idf_score(len(sentences),word,sentences)\n",
        "    tf_idf = tf_idf_score(tf,idf)\n",
        "    return tf_idf"
      ],
      "execution_count": 12,
      "outputs": []
    },
